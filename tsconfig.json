{
 "compilerOptions": {
    "module": "commonjs",
    "target": "es6",
    "noImplicitAny": true,
    "removeComments": true,
    "noUnusedParameters": false,
    "noUnusedLocals": true,
    "sourceMap": false,
    "allowJs": true
  },
  "exclude": [
    "node_modules",
    "build",
    "dist"
  ]
}
